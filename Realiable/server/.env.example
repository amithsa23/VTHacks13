# Copy this file to .env and fill in your keys. Do NOT commit real keys to git.
# Google Maps Static Maps API key (used by /api/map/:id)
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY

# Gemini/LLM provider configuration
# Set GEMINI_PROVIDER=google to use Google Generative Language (PaLM) REST API.
GEMINI_PROVIDER=google
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
GEMINI_MODEL=text-bison-001
